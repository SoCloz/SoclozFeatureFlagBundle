{% extends "SoclozFeatureFlagBundle::layout.html.twig" %}

{% block socloz_feature_flag %}
<dl>
{% for key,value in status %}
    <dt>{{ key | trans }}</dt>
    <dd>{{ value | trans({}, 'SoclozFeatureFlagBundle') }} / {{ feature_is_enabled(key) }} / {{ ab_variation(key) }}</dd>
{% endfor %}
</dl>
{% endblock %}